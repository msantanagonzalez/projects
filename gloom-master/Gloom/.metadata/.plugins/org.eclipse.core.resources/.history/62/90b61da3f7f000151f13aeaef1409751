public class Carta{
	private boolean esMuerte;
	private int espacio1;
	private int espacio2;
	private int espacio3;
	
	public Carta(){
		this.esMuerte = false;
		this.espacio1 = 0;
		this.espacio2 = 0;
		this.espacio3 = 0;
	}
	public Carta(boolean esMuerte,int espacio1, int espacio2,int espacio3){
		this.esMuerte = esMuerte;
		this.espacio1 = espacio1;
		this.espacio2 = espacio2;
		this.espacio3 = espacio3;
	}
	public boolean esMuerte(){
		return this.esMuerte;
	}
	public int getEspacio1(){
		return this.espacio1;
	}
	public int getEspacio2(){
		return this.espacio2;
	}
	public int getEspacio3(){
		return this.espacio3;
	}
}
