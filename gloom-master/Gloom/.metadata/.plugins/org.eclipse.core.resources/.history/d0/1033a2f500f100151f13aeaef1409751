public class FamiliasJuego {
	static Personaje [][] familias = {
			  {new Personaje("Pulgarcita"), new CartaPersonaje("Señor Risas"), new CartaPersonaje("Elissandre"),new CartaPersonaje("Darius"),new CartaPersonaje("Samson")}, 
			  {new Personaje("Los gemelos"), new CartaPersonaje("Goody Zarr"), new CartaPersonaje("Butterfield"), new CartaPersonaje("Lola"), new CartaPersonaje("Lord")},
			  {new Personaje("Angel"), new CartaPersonaje("Balthazar"), new CartaPersonaje("Willen"), new CartaPersonaje("Primo"), new CartaPersonaje("Dam")}, 
			  {new Personaje("Grogar"), new CartaPersonaje("Melisa"), new CartaPersonaje("Helena"), new CartaPersonaje("Slogar"), new CartaPersonaje("Elias")}
			  };
	
	public static Familia getFamilia(int i)
	{
		return new Familia(familias[i]);
	}
}
